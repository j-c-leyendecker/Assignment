#include <iostream>
using namespace std;
void nextGreaterElement(int arr[], int n, int result[]) {
    int stack[n];  
    int top = -1; 
    for (int i = n - 1; i >= 0; i--) {
        while (top != -1 && stack[top] <= arr[i]) {
            top--;
        }
        if (top != -1) {
            result[i] = stack[top];
        } else {
            result[i] = -1; 
        }
        stack[++top] = arr[i];
    }
}
int main() {
    int arr[] = {4, 5, 2, 25};
    int n = sizeof(arr) / sizeof(arr[0]);
    int result[n];
    nextGreaterElement(arr, n, result);
    for (int i = 0; i < n; i++) {
        cout << result[i] << " ";
    }
    return 0;
}
