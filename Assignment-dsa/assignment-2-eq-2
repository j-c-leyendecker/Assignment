#include <iostream>
#include <string>
using namespace std;


bool isSubstring(string big, string small) {
    int n = big.size(), m = small.size();
    if (m > n) return false;

    for (int i = 0; i <= n - m; i++) {
        int j = 0;
        while (j < m && big[i + j] == small[j]) j++;
        if (j == m) return true;
    }
    return false;
}

bool canSplit(string s) {
    int n = s.size();

    for (int i = 1; i < n - 1; i++) {       
        for (int j = i + 1; j < n; j++) {   
            string a = s.substr(0, i);
            string b = s.substr(i, j - i);
            string c = s.substr(j);

           
            if (isSubstring(b, a) && isSubstring(c, a)) return true;
            if (isSubstring(a, b) && isSubstring(c, b)) return true;
            if (isSubstring(a, c) && isSubstring(b, c)) return true;
        }
    }
    return false;
}

int main() {
    string s;
    cout << "Enter string: ";
    cin >> s;

    if (canSplit(s))
        cout << "YES, it can be split\n";
    else
        cout << "NO, it cannot be split\n";

    return 0;
}
