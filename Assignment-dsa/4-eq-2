#include <stdio.h>

int main() {
    int q[100];
    int front = 0, rear = 0;
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Enter elements: ");
    int i, j;
    for(i = 0; i < n; i++) {
        scanf("%d", &q[rear]);
        rear++;
    }
    for(i = 0; i < n; i++) {
        int min = 99999;
        int pos = -1;
        for(j = 0; j < n - i; j++) {
            int x = q[front++];
            if(x < min) {
                min = x;
                pos = j;
            }
            q[rear++] = x;
        }
        for(j = 0; j < n - i; j++) {
            int x = q[front++];
            if(j != pos)
                q[rear++] = x;
        }

        q[rear++] = min;
    }
    printf("Sorted: ");
    while(front < rear) {
        printf("%d ", q[front++]);
    }
    return 0;
}
